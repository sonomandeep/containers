### APP
NODE_ENV=development
PORT=8080
LOG_LEVEL=info
APP_URL=http://localhost:3000
UPLOAD_DIR=/tmp/containers-uploads
AUTH_CROSS_SUBDOMAIN_COOKIES_ENABLED=false
AUTH_CROSS_SUBDOMAIN_COOKIES_DOMAIN=.example.com

### DATABASE
DATABASE_URL=postgres://user:password@localhost:5432/containers

### EMAIL (SMTP)
EMAIL_FROM="Containers <no-reply@example.com>"
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USERNAME=your_smtp_user
SMTP_PASSWORD=your_smtp_password
